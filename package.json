{
  "name": "active-template",
  "version": "0.1.0",
  "packageVersion": "1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:test": "vue-cli-service build --mode test",
    "build:pre": "vue-cli-service build --mode pre",
    "build": "vue-cli-service build",
    "build:insert": "cross-env PACKAGE_TYPE=insert vue-cli-service build --dest ./insert",
		"build:offline": "cross-env PACKAGE_TYPE=offline vue-cli-service build --dest ./offline",
    "offlineZip": "fest pack build -c",
    "sentry": "cross-env USE_SENTRY=true vue-cli-service build",
    "lint": "vue-cli-service lint",
    "report": "vue-cli-service build --report",
    "browserslist": "npx browserslist"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "core-js": "^3.6.5",
    "normalize.css": "^8.0.1",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@shareit/replace-string-webpack-plugin": "^1.0.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "postcss": "^8.3.6",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.2.1",
    "sass": "^1.43.4",
    "sass-loader": "^8.0.2",
    "vconsole": "^3.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack-spritesmith": "^1.1.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
