{
  "name": "shareit-vite-template",
  "private": true,
  "version": "0.0.1",
  "keywords": [
    "eslint",
    "stylelint",
    "commitlint",
    "prettier",
    "vite",
    "vue"
  ],
  "author": {
    "name": "lucida",
    "email": "chenlei@ushareit.com"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.ushareit.me/chenlei/shareit-vite-template.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^2.7.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^1.1.2",
    "autoprefixer": "^10.4.7",
    "postcss-pxtorem": "^6.0.0",
    "sass": "^1.54.0",
    "vite": "^3.0.0",
    "vite-plugin-externals": "^0.5.1",
    "@babel/eslint-parser": "^7.16.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.5.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "@mattuy/text-frame": "^0.1.0",
    "page-flip": "^2.0.7",
    "vconsole": "^3.9.1",
    "vue-pull-to": "^0.1.8",
    "normalize.css": "^8.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{vue,html,js,mjs,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{css,scss}": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}
